{
  "name": "prettier-presentation",
  "version": "1.0.0",
  "description": "Prettier presentation",
  "main": "index.js",
  "author": "Alexi Chepura",
  "license": "MIT",
  "scripts": {
    "build:app": "webpack --config webpack.config.js",
